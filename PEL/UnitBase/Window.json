{
    "Type" : "Window",

    "ShowAttributeList" : ["备注","名称","标题","左边","顶边","宽度","高度","宽度可调整","高度可调整","可视"],
    "备注" : "",
    "名称" : "_窗口",
    "标题" : "窗口",
    "左边" : "20",
    "顶边" : "20",
    "宽度" : "400",
    "高度" : "300",
    "宽度可调整" : "True&&&False",
    "高度可调整" : "True&&&False",
    "可视" : "True&&&False",

    "PrivateAttributeList" : [],

    "CreatScript" : [
        "{{名称}} = Tk()",
        "{{名称}}.title('{{标题}}')",
        "{{名称}}.geometry('{{宽度}}x{{高度}}+{{左边}}+{{顶边}}')",
        "{{名称}}.resizable({{宽度可调整}},{{高度可调整}})"
    ]
}